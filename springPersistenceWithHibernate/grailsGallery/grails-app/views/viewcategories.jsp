<%@ page contentType="text/html" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html><head><title>Barbara Israel Fine Arts</title><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"><link href="bifa.css" rel="stylesheet" type="text/css"></head><body><div id="container">   <div id="intheader"></div>  <div id="intnav">     <div class="intnav"><a href="/viewCategories.art"><img src="images/int_galleries.gif" width="165" height="31"></a></div>     <div class="intnav"><a href="/virtualExhibitions.art"><img src="images/int_exhib.gif"></a></div>    <div class="intnav"><a href="bio.html"><img src="images/int_bio.gif"></a></div>  </div>  <div id="intnavline"></div>  <div id="intgallery">     <div class="breadcrumb">Galleries</div>    <div class="clear"> </div>    <div class="categorybox">       <div class="centertext">Select a gallery below:</div>        <c:forEach items="${art.categories}" var="curCat" varStatus="status">                <a href='/selectCategory.art?CATEGORY_ID=<c:out value="${curCat.id}"/>' >                 <div class='<c:out value="${art.posStyle[status.index]}"/>'>	  	        <div class="categoryimage"><img src="images/categoryimage1.gif"></div>		        <div class="categorytext">                    <c:out value="${curCat.categoryName}"/>             </div>	        </div>            </a>        </c:forEach>	  	  <div class="categorycenter">	  	<div class="categoryimagecenter"><img src="images/categoryimage3.gif"></div>	  	<div class="categorytext"><a href="#">Recent Works</a></div>	  </div>	  	  <div class="clear"></div>    </div>  </div>  <div class="clear"></div></div></body></html>