<%@ page contentType="text/html" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html><head><title>Barbara Israel Fine Arts</title><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"><link href="bifa.css" rel="stylesheet" type="text/css">    <script type="text/javascript" src="js/prototype.js"></script><script type="text/javascript" src="js/scriptaculous.js?load=effects"></script><script type="text/javascript" src="js/lightbox.js"></script><link rel="stylesheet" href="css/lightbox.css" type="text/css" media="screen" /></head><body><div id="container">   <div id="intheader"></div>  <div id="intnav">     <div class="intnav"><a href="/viewCategories.art"><img src="images/int_galleries.gif" width="165" height="31"></a></div>    <div class="intnav"><a href="/virtualExhibitions.art"><img src="images/int_exhib.gif"></a></div>    <div class="intnav"><a href="bio.html"><img src="images/int_bio.gif"></a></div>    <div class="leftimage"><img src="images/leftimage1.jpg"></div>    <div class="leftimage"><img src="images/leftimage2.jpg"></div>    <div class="leftimage"><img src="images/leftimage3.jpg"></div>  </div>  <div id="intnavline"></div>  <div id="intgallery">     <div class="breadcrumb"><a href="/viewCategories.art">Galleries:</a> <c:out value="${art.category.categoryName}"/></div>    <div class="clear">       <div id="gallerytextarea">         <table width="144" height="100%" border="0" align="center" cellpadding="0" cellspacing="0">          <tr>             <td align="left" valign="top"> <p> <span class="titleofpiece"><c:out value="${art.selectedArt.title}"/></span> </p>              <p class="smalltext"> <c:out value="${art.selectedArt.description}"/></p></td>          </tr>          <tr>             <td align="center" valign="bottom"><img src="images/interested.gif"></td>          </tr>        </table>      </div>      <div id="galleryimagearea">         <table width="100%" border="0" cellspacing="0" cellpadding="0">          <tr>             <td align="center" valign="top">                 <a href="displayImage.art?ARTDATA_ID=${art.selectedArt.galleryPicture.id}" rel="lightbox[${art.category.categoryName}]" title="${art.selectedArt.title}">                <img src='displayImage.art?ARTDATA_ID=${art.selectedArt.galleryPicture.id}'>                 </a>                </td>          </tr>        </table>        <div class="clear">           <table width="100%" border="0" cellspacing="0" cellpadding="4">            <tr>               <td align="center" valign="top">                  <c:if test="art.existsPrevious">                  <a href="/selectArt.art?SELECTEDART_ID=${art.previousItemId}&CATEGORY_ID=${art.category.id}">                    <img src="images/previous.gif">                  </a>                  </c:if>                  </td>              <td align="center" valign="middle"><span class="titleofpiece">                  <c:out value="${art.selectedArt.title}"/>              </span></td>              <td align="center" valign="top">                  <c:if test="art.existsNext">                  <a href='/selectArt.art?SELECTEDART_ID=<c:out value="${art.nextItemId}" />&CATEGORY_ID=<c:out value="${art.category.id}" />' >                  <img src="images/next.gif">                  </a>                  </c:if>                  </td>            </tr>          </table>        </div>      </div>    </div>    <div class="gallthumbs">       <div class="thumbscontent">         <!-- repeat this div for the thumbnails  -->        <c:forEach items="${art.artwork}" var="curEntity">          <a href="/selectArt.art?SELECTEDART_ID=${curEntity.id}&CATEGORY_ID=${art.category.id}">            <div class="thumbnail"><img src="displayImage.art?ARTDATA_ID=${curEntity.thumbnailPicture.id}"></div>            </a>            <c:if test="${!(curEntity.id == art.selectedArt.id)}">            <div style="display:none; visibility:hidden;">             <a href="displayImage.art?ARTDATA_ID=${curEntity.galleryPicture.id}" rel="lightbox[${art.category.categoryName}]" title="${curEntity.title}">            </a>            </div>            </c:if>        </c:forEach>              <!-- End repeat this div for the thumbnails  -->        <div class="clear"></div>      </div>    </div>  </div>  <div class="clear"></div></div></body></html>