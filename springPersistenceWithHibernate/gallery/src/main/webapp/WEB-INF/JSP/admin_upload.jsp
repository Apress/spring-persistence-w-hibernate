<%@ page contentType="text/html" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html><head><title>Barbara Israel Fine Arts</title><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"><link href="bifa.css" rel="stylesheet" type="text/css"><link href="admin.css" rel="stylesheet" type="text/css"></head><body><form method="post" action="/uploadArt.art" enctype="multipart/form-data"><div id="container">   <div id="intheader"></div>  <div id="intgallery">     <div class="breadcrumb">Administration Tool</div>    <div class="clear"> </div>    <div class="adminbox">       <div class="centertext">        <p>Upload Artwork </p>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Image: </div>        <div class="formobject">          <input name="storagePicture" type="file" size="20"/>        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Date: </div>        <div class="formobject">          <input name="date" type="text" size="20">        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Title: </div>        <div class="formobject">          <input name="title" type="text" size="20">        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Sub-title: </div>        <div class="formobject">          <input name="subTitle" type="text" size="20">        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Width:</div>        <div class="formobject">          <input name="width" type="text" size="20">        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Height: </div>        <div class="formobject">          <input name="height" type="text" size="20">        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Media: </div>        <div class="formobject">          <input name="media" type="text" size="20">        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Description: </div>        <div class="formobject">          <textarea name="description" cols="20" rows="8"></textarea>        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Categories:</div>        <div class="formobject">        <c:forEach items="${categories}" var="curCat">           <input name='<c:out value="${curCat.categoryName}"/>' type="checkbox" value=""></input>          <c:out value="${curCat.categoryName}"/><br>        </c:forEach>        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Exhibition:</div>        <div class="formobject">          <select name="exhibition">              <c:forEach items="${exhibits}" var="curEx">                  <option value="${curEx.id}">${curEx.exhibitionName}</option>              </c:forEach>          </select>        </div>      </div>      <div class="formline" style="margin-top: 40px">         <div class="formtext"></div>        <div class="formobject">           <a href="/createExhibition.art">Create new Exhibition</a>        </div>      </div>      <br>      <div class="formline" style="margin-top: 40px">         <div class="formtext">Accessibility:</div>        <div class="formobject">           <input name="general" type="checkbox" value=""></input>          Viewable<br>          <input name="privilege" type="checkbox" value=""></input>          Private (only accessible by the admin) </div>      </div>      <br>      <br>      <div class="formline" style="margin-top: 40px">         <div class="formtext"></div>        <div class="formobject">           <input name="cancel" type="button" id="cancel" value="Cancel and Log Off">          <input type="submit" value="Submit">        </div>      </div>      <div class="clear"> &nbsp; </div>    </div>  </div>  <div class="clear"> &nbsp; </div></div></form></body></html>